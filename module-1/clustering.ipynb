   ],
   "source": [
    "import numpy as np\n",
    "from numpy import linalg as LA\n",
    "\n",
    "# Matrix [2, 2\n",
    "#         8, 2]\n",
    "\n",
    "input = np.array([[2,2],[8,2]])\n",
    "\n",
    "eig_val, eig_vect = LA.eig(input)\n",
    "\n",
    "print(f\"Eigenvalues: {eig_val}\")\n",
    "print(f\"Eigenvectors: {eig_vect}\")"
   ]
  }
